def repeat_text(text, repeat_count):
    for _ in range(repeat_count):
        print(text)

# Input the text



text = input(" SIR YOUR TEXT: ")

# Input the number of times to repeat the text
repeat_count = int(input("REPEAT TIME: "))

# Call the function to repeat the text
repeat_text(text, repeat_count)

